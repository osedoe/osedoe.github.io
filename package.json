{
  "name": "portfolio-josediaz",
  "version": "1.0.0",
  "description": "Portfolio website for personal branding",
  "main": "app.js",
  "scripts": {
    "dev": "parcel serve src/index.html --global global --open",
    "watch": "parcel watch src/index.html",
    "build": "rm -rf build/* && parcel build src/index.pug -d build --global global --public-url /",
    "clean:cache": "rm -rf .cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
    "deploy": "git subtree push --prefix build origin gh-pages"
  },
  "keywords": [
    "parcel",
    "template",
    "html5",
    "pug",
    "css3",
    "postcss",
    "stylelint",
    "typescript",
    "tslint",
    "javascript",
    "eslint"
  ],
  "author": "Jose Diaz",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@types/jest": "^22.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3",
    "eslint": "^5.6.1",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-clean": "^1.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-font-magician": "^2.2.1",
    "postcss-mixins": "^6.2.0",
    "postcss-preset-env": "^6.0.7",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "dependencies": {}
}
